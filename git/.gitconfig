[user]
    name = Your Name
    email = you@example.com

[core]
    editor = nano
    excludesfile = ~/.gitignore_global

[color]
    ui = auto

[pull]
    rebase = true

[init]
    defaultBranch = main

[alias]
    st = status
    co = checkout
    br = branch
    ci = commit
    lg = log --oneline --graph --decorate
